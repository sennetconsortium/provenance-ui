{
  "name": "provenance-ui",
  "version": "2.5.6",
  "description": "Provenance",
  "author": "lisabruney@pitt.edu",
  "keywords": [
    "provenance",
    "ui",
    "medical",
    "dbmi"
  ],
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sennetconsortium/provenance-ui.git"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "d3": "^7.6.1",
    "jquery": "^3.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "stylus": "0.59.0",
    "sweetalert2": "^11.7.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "dist": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files",
    "css": "stylus --include-css --compress -w src/css/App.styl -o ./src/lib/ProvenanceUI.css"
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "semi": false,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.18.6",
    "babel-plugin-file-loader": "^2.0.0",
    "loglevel": "^1.8.1",
    "prettier": "^2.7.1"
  }
}
