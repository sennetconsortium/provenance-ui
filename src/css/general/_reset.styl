.clear
  clear both
*
  margin 0
  padding 0
  box-sizing border-box

html 
  font-size 16px
  line-height 22px
  
html, body
  height 100%

body
  p5()
  text-size-adjust none
  position relative

li  
  list-style-position inside

a, a:visited 
  color $col-matisse

.hide 
  hide()

